div\", _hoisted_5, [_createElementVNode(\"div\", {\n    class: \"modal\"\n  }, [_cache[4] || (_cache[4] = _createElementVNode(\"h3\", null, \"Delete Message\", -1 /* HOISTED */)), _cache[5] || (_cache[5] = _createElementVNode(\"p\", null, \"Are you sure you want to delete this message?\", -1 /* HOISTED */)), _createElementVNode(\"div\", {\n    class: \"modal-actions\"\n  }, [_createElementVNode(\"button\", {\n    onClick: $setup.confirmDelete,\n    class: \"confirm-btn\"\n  }, \"Delete\"), _createElementVNode(\"button\", {\n    onClick: $setup.cancelDelete,\n    class: \"cancel-btn\"\n  }, \"Cancel\")])])])) : _createCommentVNode(\"v-if\", true), _createElementVNode(\"ul\", _hoisted_6, [(_openBlock(true), _createElementBlock(_Fragment, null, _renderList($setup.messageList, msg => {\n    return _openBlock(), _createElementBlock(\"li\", {\n      key: msg.id,\n      class: _normalizeClass(msg.sender === $setup.currentUser?.uid ? 'sent' : 'received'),\n      onMouseover: $event => $setup.hoveredMessage = msg.id,\n      onMouseleave: _cache[0] || (_cache[0] = $event => $setup.hoveredMessage = null)\n    }, [$setup.isImageMessage(msg.message) ? (_openBlock(), _createElementBlock(\"div\", _hoisted_8, [_createElementVNode(\"img\", {\n      src: msg.message,\n      alt: \"Image\",\n      class: \"chat-image\"\n    }, null, 8 /* PROPS */, _hoisted_9)])) : (_openBlock(), _createElementBlock(\"div\", _hoisted_10, _toDisplayString(msg.message), 1 /* TEXT */)), _createElementVNode(\"small\", _hoisted_11, _toDisplayString($setup.formatTimestamp(msg.timestamp)), 1 /* TEXT */), msg.sender === $setup.currentUser?.uid && $setup.hoveredMessage === msg.id ? (_openBlock(), _createElementBlock(\"button\", {\n      key: 2,\n      onClick: _withModifiers($event => $setup.deleteMessage(msg.id), [\"stop\"]),\n      class: \"delete-button\"\n    }, [...(_cache[6] || (_cache[6] = [_createElementVNode(\"i\", {\n      class: \"fas fa-trash\"\n    }, null, -1 /* HOISTED */)]))], 8 /* PROPS */, _ho